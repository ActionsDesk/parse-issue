{
  "name": "parse-issue",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub Action for parsing GitHub Issues",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonmacgowan/parse-issue.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Jason Macgowan",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.22",
    "@typescript-eslint/parser": "^2.14.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^26.1.0",
    "jest-circus": "^26.1.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^26.1.4",
    "typescript": "^3.7.4"
  }
}
